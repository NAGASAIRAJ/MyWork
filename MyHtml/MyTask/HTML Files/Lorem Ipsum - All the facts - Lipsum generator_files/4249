    ]),

    hasSVG_44782=!!document.createElementNS&&!!document.createElementNS("http://www.w3.org/2000/svg","svg").createSVGRect,
    hasRGBA_44782=supportsRGBA_44782(),
    hasJSON_44782=window.JSON&&window.JSON.parse&&window.JSON.stringify,

     function getInternetExplorerVersion_44782() {
         // Returns the version of Internet Explorer or a -1
         // (indicating the use of another browser).

             var rv = -1; // Return value assumes failure.
             if (navigator.appName == 'Microsoft Internet Explorer') {
                 isIEBrowser_44782=true;
                 var ua = navigator.userAgent;
                 var re  = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");

                 if (re.exec(ua) != null)
                     rv = parseFloat( RegExp.$1 );
             }

             return rv;
         }

      //returns true if ie version is less than 9, say ie6, ie7, ie8
         // -1 for non IE browsers.
         function isIEBrowserWithVersionLessThan9_44782 () {

             browserVersion_44782 = getInternetExplorerVersion_44782();  //-1 for non IE browsers
             if((browserVersion_44782 != -1) && (browserVersion_44782 < 9)) {
                 return true;

             }
             return false;
         }

    //code to detect Edge Features, courtesy  (http://dl.dropboxusercontent.com/u/13483458/test-edge.html)
    var testEle_44782=document.createElement("div_44782");
    function isSupported_44782(a){

        var d=testEle_44782.style,e;
        for(i=0;i<a.length;i++)
            if(e=a[i],d[e]!==void 0)
                return!0;
        return!1
    }

    function supportsRGBA_44782(){

        testEle_44782.cssText="background-color:rgba(150,255,150,.5)";
        if((""+testEle_44782.style.backgroundColor).indexOf("rgba")==0)
            return!0;
        return!1
    }

    var hasTransform_44782=isSupported_44782([
        "transformProperty",

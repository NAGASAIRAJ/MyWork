    function isIEBrowserVersion9_44782() {
        return (isIEBrowser_44782 && (browserVersion_44782 == 9)) ? true : false;
    }

    function isEdgeSupported_44782() {
        if(isIEBrowserVersion9_44782()) {
            return "y";           //hardcoding IE9 edge support.
        }
        if(hasTransform_44782) {
            if(requiresSVG_44782&&!hasSVG_44782)
                return "f";
            return "y";
        }
        return "f";
    }

    function isCanvasSupported_44782(){
      var elem = document.createElement('canvas');
      return !!(elem.getContext && elem.getContext('2d'));
    }

    function isHTML5FeaturesSupported_44782() {
         return (isCanvasSupported_44782()) ? "y" : "f";
    }

    var requiresSVG_44782=false;
    //edge detection code end

